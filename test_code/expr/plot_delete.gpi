unset title
set terminal postscript eps enhanced monochrome size 20cm, 10cm rounded font "Helvetical, 46"
set style data histogram
set style histogram clustered gap 1 title offset 0, -1
set style fill pattern border -1
set boxwidth 1 absolute
set key off
#set key enhanced outside Left top horizontal reverse nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
#set key enhanced outside Left top vertical reverse nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
#set key enhanced outside center top horizontal reverse nobox samplen 2 width -.7 height 0.1 spacing 1.5 maxrows 1 font "Helvetical, 23"
#set key enhanced outside left top horizontal reverse samplen 2 width -1 spacing 1 font "Helvetica, 23"

set bmargin 3
set xrange [.3:9.2]
set xtics  nomirror font "Helvetical, 46"
set yrange [0:70]
set ytics autofreq 20 nomirror offset 0.5 font "Helvetical, 46"
set ytic 20
set grid ytics

set ylabel "Transaction/sec." offset 2 font "Helvetical, 46"

set output "img/MFAW_delete.eps"
plot newhistogram "840 Pro" at 1, 'oper_delete.dat'u "840EP":xtic(1) fill pattern 0 lt 1 t 'EXT4 PERSIST', ''u "840EW":xtic(1) fill pattern 1 lt 1 t 'EXT4 WAL', ''u "840EL":xtic(1) fill pattern 5 lt 1 t 'EXT4 LS-MVBT',''u "840FP":xtic(1) fill pattern 2 lt 1 t 'F2FS PERSIST', ''u "840FW":xtic(1) fill pattern 4 lt 1 t 'F2FS WAL', ''u "840FL":xtic(1) fill pattern 6 lt 1 t 'F2FS LS-MVBT', \
newhistogram "950 Pro" at 5.5, 'oper_delete.dat'u "950EP":xtic(1) fill pattern 0 lt 1 notitle, ''u "950EW":xtic(1) fill pattern 1 lt 1 notitle, ''u "950EL":xtic(1) fill pattern 5 lt 1 notitle, ''u "950FP":xtic(1) fill pattern 2 lt 1 notitle, ''u "950FW":xtic(1) fill pattern 4 lt 1 notitle, ''u "950FL":xtic(1) fill pattern 6 lt 1 notitle
