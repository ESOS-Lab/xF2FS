unset title
set terminal postscript eps enhanced monochrome size 8cm, 4.5cm font "Helvetical, 22"
#set style data histogram
#set style histogram clustered gap 1 title offset 0, -1
set style fill pattern border -1
#set boxwidth 1 absolute
#set key enhanced Right top horizontal reverse nobox samplen 2 width -.7 height .1 spacing 1.5 font "Helvetical, 22"

set key off

set boxwidth 0.75
#set style fill solid

set yrange [0:420]
set grid ytics
set ytics auto 100

set ylabel "Write Volume (MB)" font "Helvetical, 24"

set output "atomic_write.eps"
