unset title
set terminal postscript eps enhanced monochrome size 20cm, 8cm rounded font "Helvetical, 46"
set style data histogram
set style histogram clustered gap 1 title offset 0, -1
set style fill pattern border -1
set boxwidth 1 absolute
set key enhanced inside right top horizontal nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
#set key enhanced outside Left top vertical reverse nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
#set key enhanced outside center top horizontal reverse nobox samplen 2 width -.7 height 0.1 spacing 1.5 maxrows 1 font "Helvetical, 23"
#set key enhanced outside left top horizontal reverse samplen 2 width -1 spacing 1 font "Helvetica, 23"
#set key off

set bmargin 3
set xrange [-0.7:4.7]
set xtics  nomirror font "Helvetical, 46"
set yrange [0:5000]
set ytics autofreq 1000 nomirror offset 0.5 font "Helvetical, 46"
#set ytic 20
set grid ytics

set ylabel "Transaction/sec." offset 2 font "Helvetical, 46"

set output "img/tps_843tn_insert_4core.eps"
plot '843tn.dat'u "exF2FS":xtic(1) fill pattern 0 lt 1 t "exF2FS", ''u "TxFS":xtic(1) fill pattern 1 lt 1 t "TxFS"
