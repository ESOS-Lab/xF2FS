unset title
set terminal postscript eps enhanced monochrome size 12cm, 8cm rounded font "Helvetical, 26"
#set terminal postscript eps enhanced monochrome rounded
set style data histogram
set style histogram clustered gap 1 title offset 0, -1
set style fill pattern border -1
set boxwidth 1 absolute
#set key enhanced inside right top horizontal nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
#set key enhanced inside right top vertical nobox samplen 2 width 2 height .1 spacing 1.5 font "Helvetical, 26"
set key outside top center horizontal font "Helvetical, 24"

set bmargin 3
#set xrange [-0.5:0.5]
#set xtics nomirror font "Helvetical, 46"
#set xtics ("EXT4 PERSIST" 1, "EXT4 WAL" 2, "F2FS PERSIST" 3, "F2FS WAL" 4, "F2FS Atomic" 5, "F2FS Durable Atomic" 6) rotate by -45
#unset xtics
unset xtics
set xtics ("PERSIST" 0.1)
set yrange [0:450]
set ytics autofreq 100
set grid ytics

set ylabel "Transaction/sec." offset 1

set output "img/effect_of_datomic.eps"
