plot newhistogram "EXT4" at 0.1,\
     '850pro.dat' u "EP":xtic(1) fill pattern 0 lt 1,\
     newhistogram "F2FS" at 1,\
               '' u "FP":xtic(1) fill pattern 2 lt 1,\
               '' u "N3":xtic(1) fill pattern 0 lt 1 notitle,\
               '' u "FA":xtic(1) fill pattern 4 lt 1,\
               '' u "N5":xtic(1) fill pattern 0 lt 1 notitle,\
               '' u "FD":xtic(1) fill pattern 5 lt 1
