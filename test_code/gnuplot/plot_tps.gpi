unset title
set terminal postscript eps enhanced monochrome size 8cm, 8cm rounded font "Helvetical, 18"
set style data histogram
set style histogram clustered gap 1 title offset 0, -1
set style fill pattern border -1
set boxwidth 1 absolute
set key enhanced outside Left top horizontal reverse nobox samplen 2 width -.7 height .1 spacing 1.5 font "Helvetical, 17"

set bmargin 3
set xrange [.3:3.2]
set xtics  nomirror font "Helvetical, 18"
set yrange [0:120]
set ytics autofreq 20 nomirror font "Helvetical, 22"
set grid ytics

set ylabel "Transaction/sec." font "Helvetical, 24"

set output "img/tps.eps"
plot newhistogram "840 Pro" at 1, 'tps.dat' u "840EP":xtic(1) fill pattern 0 lt 1 t 'EXT4 PERSIST', '' u "840EW":xtic(1) fill pattern 1 lt 1 t 'EXT4 WAL', ''u "840FP":xtic(1) fill pattern 2 lt 1 t 'F2FS PERSIST', ''u "840FW":xtic(1) fill pattern 3 lt 1 t 'F2FS WAL', ''u "840FM":xtic(1) fill pattern 4 lt 1 t 'F2FS MAW', \
	newhistogram "950 Pro" at 2.5, 'tps.dat' u "950EP":xtic(1) fill pattern 0 lt 1 notitle, '' u "950EW":xtic(1) fill pattern 1 lt 1 notitle, ''u "950FP":xtic(1) fill pattern 2 lt 1 notitle, ''u "950FW":xtic(1) fill pattern 3 lt 1 notitle, ''u "950FM":xtic(1) fill pattern 4 lt 1 notitle
