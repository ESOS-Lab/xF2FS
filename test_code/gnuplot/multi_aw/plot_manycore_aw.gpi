set terminal postscript eps enhanced monochrome size 12cm,4cm linewidth 3 font "Helvetica, 22"

set key right bottom

set yrange [0:2]
set ylabel "Latency (sec)"
set ytics autofreq 0.4

set xrange [0:9]
set xtics 1
set xlabel "# of Files or Threads"

set output "manycore_aw.eps"

plot 'manycore_aw.dat' using 1:($2/1000) title "single" with linespoints pt 1,\
	'' using 1:($3/1000) title "multi" with linespoints pt 2
