	kirin_pcie_rc@0xf4000000 {
		board_type = <0>;
		wl_power = <31>;

		/*EP Device Description: here describe BCM 4359*/
		ep_vend_id = <0x14e4>;
		ep_dev_id = <0x4355>;
		ep_ltr_latency = <0x10031003>;
		ep_l1ss_ctrl2 = <0x61>;
